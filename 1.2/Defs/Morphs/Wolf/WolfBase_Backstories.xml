		<slot>Adulthood</slot>
		<bodyTypeFemale>Hulk</bodyTypeFemale>
		<bodyTypeMale>Hulk</bodyTypeMale>
		<spawnCategories>
			<li>MutantWanderer</li>
			<li>FreeMutant</li>
		</spawnCategories>
		<skillGains>
			<li>
				<defName>Melee</defName>
				<amount>5</amount>
			</li>
			<li>
				<defName>Social</defName>
				<amount>3</amount>
			</li>
			<li>
				<defName>Intellectual</defName>
				<amount>-2</amount>
			</li>
		</skillGains>
		<modExtensions>
