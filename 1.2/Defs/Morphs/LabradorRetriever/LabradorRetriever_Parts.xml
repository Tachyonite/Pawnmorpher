<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name="LabradorRetrieverPart" ParentName="PawnmorphPart" Abstract="true">
		<classInfluence>LabradorRetrieverMorph</classInfluence>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="LabradorRetrieverPart">
		<defName>EtherLabradorRetrieverJaw</defName>
		<label>labrador snout</label>
		<description>The forever smiling snout of a labrador.</description>
		<mutationTale>GrewMuzzleMisc</mutationTale>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.40</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.40</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>adapted</label>
				<minSeverity>1</minSeverity>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.08</offset>
					</li>
					<li>
						<capacity>Eating</capacity>
						<offset>0.40</offset>
					</li>
				</capMods>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>teeth</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>10</power>
						<cooldownTime>2</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
			<li Class="Pawnmorph.Hediffs.CompProperties_MutationSeverityAdjust">
				<severityPerDay>0.25</severityPerDay>
			</li>
		</comps>
		<parts>
			<li>Jaw</li>
		</parts>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="LabradorRetrieverPart">
		<defName>EtherLabradorRetrieverEar</defName>
		<label>labrador ear</label>
		<description>A floppy canine ear that wobbles all over the place.</description>
		<mutationTale>EarsChangedMisc</mutationTale>
		<parts>
			<li>Ear</li>
		</parts>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="LabradorRetrieverPart">
		<defName>EtherLabradorRetrieverTail</defName>
		<label>labrador tail</label>
		<description>The briskly wagging, excitable tail of a labrador retriever.</description>
		<mutationTale>GrewTail</mutationTale>
		<parts>
			<li>Tail</li>
			<li>MorphTail</li>
		</parts>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>