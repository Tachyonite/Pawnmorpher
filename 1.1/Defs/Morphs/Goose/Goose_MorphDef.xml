<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SensitiveFoodMorph">
		<defName>GooseMorph</defName>
		<label>goosemorph</label>
		<description>A human who now shares many features with geese due to exposure to mechanite mutagens. They are usually coated in white feathers and have large goose beaks. They can be very loud.</description>
		<race>Goose</race>
		<classification>Anseriform</classification>
		<categories>
			<li>Farm</li>
			<li>Avian</li>
		</categories>
		<fullTransformation>PawnmorphGooseTF</fullTransformation>
		<partialTransformation>PawnmorphGooseTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(255,255,255)</skinColorOverride>
				<femaleSkinColorOverride>(255,255,255)</femaleSkinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>GooseTransformationMemory</transformationMemory>
			<revertedMemory>GooseRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>