<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SensitiveFoodMorph">
		<defName>EmuMorph</defName>
		<label>emumorph</label>
		<description>A human who now shares many features with emus due to exposure to mechanite mutagens. Usually found covered in feathers, and stubby arms more resembling wings. They are great runners.</description>
		<race>Emu</race>
		<classification>Ratite</classification>
		<categories>
			<li>Arid</li>
			<li>Avian</li>
		</categories>
		<fullTransformation>PawnmorphEmuTF</fullTransformation>
		<partialTransformation>PawnmorphEmuTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(105,90,78)</skinColorOverride>
				<femaleSkinColorOverride>(105,90,78)</femaleSkinColorOverride>
				<hairColorOverride>(33,32,30)</hairColorOverride>
				<femaleHairColorOverride>(33,32,30)</femaleHairColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>EmuTransformationMemory</transformationMemory>
			<revertedMemory>EmuRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>