	<!-- Muzzle - Suggestion: 50% of stats for adapting -->
	
	<Pawnmorph.Hediffs.MutationDef Name ="PawnmorphJawPart" ParentName="PawnmorphPart" Abstract="true">
		<description>A morph muzzle.</description>
		<mutationTale>GrewMuzzleMisc</mutationTale>
		<minSeverity>0</minSeverity>
		<initialSeverity>0</initialSeverity>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>initial</key>
				<label>initial</label>
				<minSeverity>0</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>pressing</key>
				<label>pressing out</label>
				<minSeverity>0.1</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>growing</key>
				<label>growing</label>
				<minSeverity>0.3</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>adapting</key>
				<label>adapting</label>
				<minSeverity>0.5</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>adapted</key>
				<label>adapted</label>
				<minSeverity>1</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>paragon</key>
				<label>paragon</label>
				<minSeverity>2.5</minSeverity>
			</li>
		</stages>
		<comps>
			<li Class="Pawnmorph.Hediffs.CompProperties_MutationSeverityAdjust">
				<severityPerDay>0.25</severityPerDay>
			</li>
		</comps>
		<parts>
			<li>Jaw</li>
		</parts>
		<categories>
			<li>Major</li>
		</categories>
	</Pawnmorph.Hediffs.MutationDef>

