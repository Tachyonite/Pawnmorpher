		<race>Boomrat</race>
		<categories>
			<li>Temperate</li>
			<li>Production</li>
		</categories>
		<fullTransformation>PawnmorphBoomratTF</fullTransformation>
		<partialTransformation>PawnmorphBoomratTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(105,78,68)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>BoomratTransformationMemory</transformationMemory>
			<revertedMemory>BoomratRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>