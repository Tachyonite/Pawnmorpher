	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherDextrousPawHand</defName>
		<stages>
			<li>
				<label>Rodent hands</label>
				<description>These paws sure are good at picking up little things off the ground.</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Rodent hands</label>
				<description>They're so dainty and dextrous, these are better than my old hands!</description>
				<visible>true</visible>
				<baseMoodEffect>3</baseMoodEffect>
			</li>
			<li>
				<label>Rodent hands</label>
				<description>Ahhhhh! They're so wrinkly! Get them off me!</description>
				<visible>true</visible>
				<baseMoodEffect>-3</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>

