    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherRodentJaw</defName>
        <stages>
            <li>
                <label>rodent face</label>
                <description>What's up with these teeth?</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>rodent face</label>
                <description>I can eat whatever I want!</description>
                <visible>true</visible>
                <baseMoodEffect>2</baseMoodEffect>
            </li>
            <li>
                <label>rodent face</label>
                <description>God, I look so ugly with this thing...</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherRodentEar</defName>
        <stages>
            <li>
                <label>rodent ears</label>
                <description>Why are my ears so small?</description>
                <visible>True</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>rodent ears</label>
                <description>These tiny ears are so cute!</description>
                <visible>True</visible>
                <baseMoodEffect>1</baseMoodEffect>
            </li>
            <li>
                <label>rodent ears</label>
                <description>Does anyone have a hat I can hide these things under?</description>
                <visible>True</visible>
                <baseMoodEffect>-2</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherRodentTail</defName>
		<stages>
			<li>
				<label>rodent tail</label>
				<description>It trails behind me uselessly...</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>rodent tail</label>
				<description>I wish it was better at something...</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>rodent tail</label>
				<description>I feel dirty and like a thief, like this is a punishment...</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>

</Defs>