<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../Schemas/ThoughtDef.xsd">

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherFelineJaw</defName>
		<stages>
			<li>
				<label>feline face</label>
				<description>It's hard to talk without purring.</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>feline face</label>
				<description>Rrowwl!</description>
				<visible>true</visible>
				<baseMoodEffect>2</baseMoodEffect>
			</li>
			<li>
				<label>feline face</label>
				<description>No! I don't want to purr!</description>
				<visible>true</visible>
				<baseMoodEffect>-3</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherFelineEar</defName>
		<stages>
			<li>
				<label>feline ear</label>
				<description>Ouch, these things can hear everything.</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>feline ear</label>
				<description>I can hear everything!</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>feline ear</label>
				<description>I feel like an anime character...</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherFelineTail</defName>
		<stages>
			<li>
				<label>feline tail</label>
				<description>This tail keeps getting in the way, but it's good for balance.</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>feline tail</label>
				<description>Aww! It's so cute!</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>feline tail</label>
				<description>I keep tripping over this stupid tail!</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherFelineEye</defName>
		<stages>
			<li>
				<label>feline eyes</label>
				<description>My eyes feel strange, but I can see better than I ever could before.</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>feline eyes</label>
				<description>I can see in the dark better than ever, and my eyes look so mysterious!</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>feline eyes</label>
				<description>These slit pupils make me look unnatural.</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherFelineClaws</defName>
		<stages>
			<li>
				<label>Claws</label>
				<description>Yowtch! Poked myself again.</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Claws</label>
				<description>Claws are so useful, I'll never need a knife again!</description>
				<visible>true</visible>
				<baseMoodEffect>2</baseMoodEffect>
			</li>
			<li>
				<label>Claws</label>
				<description>I keep poking holes in my clothes with these unruly claws!</description>
				<visible>true</visible>
				<baseMoodEffect>-3</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
</Defs>
