			</li>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<capMods>
						<li>
							<capacity>Hearing</capacity>
							<offset>+0.05</offset>
						</li>
					</capMods>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

