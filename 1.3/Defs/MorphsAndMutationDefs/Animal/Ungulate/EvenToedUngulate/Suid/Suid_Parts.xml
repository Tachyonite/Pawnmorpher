		<stagePatches>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<capMods>
						<li>
							<capacity>Metabolism</capacity>
							<offset>-0.025</offset>
						</li>
					</capMods>
				</values>
			</li>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<capMods>
						<li>
							<capacity>Metabolism</capacity>
							<offset>-0.05</offset>
						</li>
					</capMods>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

</Defs>