					<label></label>
					<statOffsets>
						<PawnBeauty>+1.0</PawnBeauty>
					</statOffsets>
					<verbOverrides>
						<li>
							<label>horns</label>
							<chanceFactor>1</chanceFactor>
							<power>13.5</power>
