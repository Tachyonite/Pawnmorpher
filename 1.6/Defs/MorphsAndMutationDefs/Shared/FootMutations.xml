	<Pawnmorph.Hediffs.MutationDef ParentName="PawnmorphFootPart">
		<defName>EtherMonkeyFoot</defName>
		<label>dextrous toes</label>
		<description>A foot that looks almost like a hand</description>
		<classInfluence>Simian</classInfluence>
		<!-- <mutationTale></mutationTale> -->
