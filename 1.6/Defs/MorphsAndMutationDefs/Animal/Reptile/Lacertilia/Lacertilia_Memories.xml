<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../Schemas/ThoughtDef.xsd">

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherLacertiliaSnout</defName>
		<stages>
			<li>
				<label>Lizard face</label>
				<description>It's so dry and rough...</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Lizard face</label>
				<description>I look like a raptor! Fear me!</description>
				<visible>true</visible>
				<baseMoodEffect>2</baseMoodEffect>
			</li>
			<li>
				<label>Lizard face</label>
				<description>Ew! I have to eat with this gross, scaly thing!</description>
				<visible>true</visible>
				<baseMoodEffect>-3</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherLacertiliaTail</defName>
		<stages>
			<li>
				<label>Lizard tail</label>
				<description>This thing gets stuck in doors often, but at least it helps with my balance a bit.</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Lizard tail</label>
				<description>I can use this to whip people like a real lizard! Maybe it will help me swim too...</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Lizard tail</label>
				<description>Do lizard tails regrow if I were to get this thing removed?</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>

</Defs>
